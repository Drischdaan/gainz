{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@gainz-api/feature-correlation(|/.*)$": "<rootDir>/../libs/features/feature-correlation/src/$1",
    "^@gainz-api/feature-database(|/.*)$": "<rootDir>/../libs/features/feature-database/src/$1"
  }
}